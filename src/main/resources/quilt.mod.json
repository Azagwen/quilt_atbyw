{
  "schema_version": 1,
  "quilt_loader": {
    "group": "net.azagwen",
    "id": "atbyw",
    "version": "${version}",
    "metadata": {
      "name": "All The Blocks You Want (ATBYW)",
      "description": "All The Blocks You Want is a Minecraft Fabric mod that aims to make Minecraft's block lineup more rich and varied, it adds building blocks, utility blocks and redstone components of all sorts.",
      "contributors": {
        "Azagwen": "Owner & Main Dev"
      },
      "contact": {
        "homepage": "https://www.curseforge.com/minecraft/mc-mods/atbyw",
        "sources": "https://github.com/Azagwen/ATBYW",
        "issues": "https://github.com/Azagwen/ATBYW/issues"
      },
      "license": "MIT",
      "icon": "assets/atbyw/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "net.azagwen.atbyw.AtbywMain",
      "client": "net.azagwen.atbyw.client.AtbywClient",
      "dynamiclights": "net.azagwen.atbyw.DynamicLights"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.16.0-"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=1.0.0-"
      },
      {
        "id": "minecraft",
        "versions": ">=1.18.2"
      }
    ]
  },
  "mixin": "test.mixins.json"
}
